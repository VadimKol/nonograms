{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "airbnb-base",
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "no-plusplus": "off",
      "no-console": "warn",
      "max-len": ["warn", {"code": 120}],
      "indent": ["warn", 2, {"SwitchCase": 1}],
      "import/prefer-default-export": "off",
      "no-param-reassign": ["error", {"props": false}]
    }
}
